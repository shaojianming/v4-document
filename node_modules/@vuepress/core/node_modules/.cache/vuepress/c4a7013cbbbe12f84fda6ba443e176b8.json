Pagination, CategoriesBar },\n  mounted () {\n    const queryCategory = this.$route.query.category\n    if (queryCategory) {\n      this.category = queryCategory\n      this.total = this.$groupPosts.categories[queryCategory].length\n    } else {\n      this.total = this.$sortPosts.length\n    }\n    if (this.$route.query.p) {\n      this.currentPage = Number(this.$route.query.p)\n    }\n\n    // 滚动条定位到当前分类（增强用户体验）\n    const cateEl = document.querySelector('.categories')\n    if (cateEl) {\n      setTimeout(() => {\n        const activeEl = cateEl.querySelector('.active')\n        const topVal = activeEl ? activeEl.offsetTop : 0\n        cateEl.scrollTo({ top: topVal, behavior: 'smooth' })\n      }, 300)\n    }\n  },\n  methods: {\n    handlePagination (i) { // 分页\n      this.currentPage = i\n    }\n  },\n  watch: {\n    '$route.query.category' (category) {\n      this.category = category ? decodeURIComponent(category) : ''\n      if (this.category) {\n        this.total = this.$groupPosts.categories[this.category].length\n      } else {\n        this.total = this.$sortPosts.length\n      }\n      this.currentPage = 1\n\n    }\n  }\n}\n"],"mappings":"AACA,OAAOA,UAAU,MAAM,8BAA8B;AACrD,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,OAAOC,UAAU,MAAM,8BAA8B;AACrD,OAAOC,aAAa,MAAM,iCAAiC;AAE3D,eAAe;EACbC,IAAIA,CAAA,EAAI;IACN,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,CAAC;MAAE;MACVC,OAAO,EAAE,EAAE;MAAE;MACbC,WAAW,EAAE,CAAC;IAChB,CAAC;EACH,CAAC;;EACDC,UAAU,EAAE;IAAET,UAAU;IAAEC,QAAQ;IAAEC,UAAU;IAAEC;EAAc,CAAC;EAC/DO,OAAOA,CAAA,EAAI;IACT,MAAMC,aAAa,GAAG,IAAI,CAACC,MAAM,CAACC,KAAK,CAACR,QAAQ;IAChD,IAAIM,aAAa,EAAE;MACjB,IAAI,CAACN,QAAQ,GAAGM,aAAa;MAC7B,IAAI,CAACL,KAAK,GAAG,IAAI,CAACQ,WAAW,CAACC,UAAU,CAACJ,aAAa,CAAC,CAACK,MAAM;IAChE,CAAC,MAAM;MACL,IAAI,CAACV,KAAK,GAAG,IAAI,CAACW,UAAU,CAACD,MAAM;IACrC;IACA,IAAI,IAAI,CAACJ,MAAM,CAACC,KAAK,CAACK,CAAC,EAAE;MACvB,IAAI,CAACV,WAAW,GAAGW,MAAM,CAAC,IAAI,CAACP,MAAM,CAACC,KAAK,CAACK,CAAC,CAAC;IAChD;;IAEA;IACA,MAAME,MAAM,GAAGC,QAAQ,CAACC,aAAa,CAAC,aAAa,CAAC;